{
  "name": "@eco-foundation/eco-kms-signer",
  "version": "0.0.1",
  "private": true,
  "description": "Modern Eco KMS Signer Library - (Based on web3-kms-signer by Jonathan Okz)",
  "license": "MIT",
  "author": "Eco Engineering <eng@eco.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/eco/eco-kms-signer.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r dev",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "typecheck": "pnpm -r typecheck",
    "clean": "pnpm -r clean",
    "release": "changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitest/coverage-v8": "^1.3.1",
    "@vitest/ui": "^1.3.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  },
  "pnpm": {
    "overrides": {
      "typescript": "^5.3.3"
    }
